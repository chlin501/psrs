#!/bin/sh

echo "stop all processes ..."
jps | grep sbt-launch | awk '{print $1}' | xargs kill -9

log=$1
output=$2
if [ "x" = "x$log" ]; then
  log="/tmp/psrs/logs"
  output="/tmp/psrs/output"
fi

if [ -d $log ]; then
  echo "remove log directory: $log"
  rm -rf $log
  echo "remove output directory: $output"
  rm -rf $output
fi
